<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox style="-fx-background-color: #bc7a00;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EditController">
   <children>
      <HBox>
         <children>
            <JFXTextField focusColor="WHITE" maxWidth="170.0" minWidth="170.0" prefWidth="170.0" style="-fx-background-color: white;">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" top="3.0" />
               </HBox.margin>
            </JFXTextField>
            <JFXTextField focusColor="WHITE" layoutX="10.0" layoutY="10.0" maxWidth="170.0" minWidth="170.0" prefWidth="170.0" style="-fx-background-color: white;">
               <HBox.margin>
                  <Insets right="10.0" top="3.0" />
               </HBox.margin>
            </JFXTextField>
            <Region HBox.hgrow="ALWAYS" />
            <JFXButton style="-fx-border-radius: .5em; -fx-border-color: white; -fx-border-width: 2;" text="Submit Changes" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton layoutX="562.0" layoutY="12.0" style="-fx-border-radius: .5em; -fx-border-color: white; -fx-border-width: 2;" text="Cancel" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </JFXButton>
         </children>
         <padding>
            <Insets bottom="2.0" top="2.0" />
         </padding>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <JFXTextArea focusColor="WHITE" prefHeight="90.0" prefWidth="350.0" style="-fx-background-color: white;">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </HBox.margin>
            </JFXTextArea>
            <Region HBox.hgrow="ALWAYS" />
            <BorderPane fx:id="timeBorderPane" style="-fx-background-color: white; -fx-background-radius: .5em;">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </BorderPane>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" top="5.0" />
   </padding>
</VBox>
